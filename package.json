{
  "name": "lukekarrys.com",
  "description": "lukekarrys.com",
  "version": "1.0.0",
  "bugs": {
    "url": "https://github.com/lukekarrys/lukekarrys.com/issues"
  },
  "dependencies": {
    "tailwindcss": "^2.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.4",
    "cpr": "^3.0.1",
    "cssnano": "^4.1.10",
    "mkdirp": "^1.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.14",
    "postcss-cli": "^8.3.0",
    "postcss-import": "^13.0.0",
    "prettier": "^2.2.1"
  },
  "homepage": "https://github.com/lukekarrys/lukekarrys.com#readme",
  "keywords": [],
  "license": "ISC",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lukekarrys/lukekarrys.com.git"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:css": "postcss src/styles.css -o build/styles.css",
    "build:html": "cpr src/*.html build/ -o",
    "build:assets": "cpr assets build -o",
    "dev": "nodemon --exec 'npm run build' --ignore 'build/' -e 'html,css,js'",
    "prebuild": "mkdirp build"
  }
}
