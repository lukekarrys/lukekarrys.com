{
  "name": "lukekarrys.com",
  "description": "My personal site.",
  "version": "1.0.0",
  "dependencies": {
    "colors-less": "^1.0.0",
    "colors.css": "^3.0.0",
    "dom-events": "^0.1.1",
    "geopattern": "^1.2.3",
    "waypoints": "^4.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.13.2",
    "babelify": "^7.3.0",
    "connect-livereload": "^0.5.4",
    "git-directory-deploy": "^1.5.0",
    "grunt": "^1.0.1",
    "grunt-browserify": "^5.0.0",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-connect": "^1.0.2",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-cssmin": "^1.0.1",
    "grunt-contrib-jade": "^1.0.0",
    "grunt-contrib-less": "^1.3.0",
    "grunt-contrib-uglify": "^2.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "less-plugin-autoprefix": "^1.5.1",
    "serve-static": "^1.11.1",
    "standard": "^7.1.2"
  },
  "engines": {
    "node": "^5.12.40",
    "npm": "^3.10.5"
  },
  "license": "ISC",
  "main": "Gruntfile.js",
  "private": true,
  "scripts": {
    "build": "NODE_ENV=production grunt build",
    "codeship-setup": "npm run npm-version && npm install && npm run build",
    "codeship-test": "npm run lint && npm run validate",
    "deploy": "git-directory-deploy --directory public --username lukekarrys --email lukekarrys@gmail.com --repo git@github.com:lukekarrys/lukekarrys.com.git --message='--skip-ci'",
    "lint": "standard",
    "npm-version": "npm install -g npm@`cat package.json | jq -r '.engines.npm'`",
    "start": "NODE_ENV=development grunt serve",
    "validate": "npm ls"
  },
  "standard": {
    "ignore": [
      "public",
      "_assets"
    ]
  }
}
